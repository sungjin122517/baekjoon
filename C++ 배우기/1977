#include <iostream>
using namespace std;

int main()
{
    int M; int N;
    cin >> M >> N;

    int sum = 0;
    if (M <= N && N <= 10000)
    {
        for (int i = 1; i * i <= N; i++) {
            if (i*i >= M)
                sum += i * i;
        }
            

        if (sum == 0)
            cout << -1 << endl;
        else
        {
            cout << sum << endl;
            for (int i = 1; i * i <= N; i++)
            {
                if (i*i >= M) {
                    cout << i*i << endl;
                    break;
                }
            }

            if (sum = 0)
                cout << -1 << endl;
        }
    }

    return 0;
}
